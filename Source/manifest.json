{
	"manifest_version": 2,

	"name": "Millennials to Snake People",
    	"short_name": "Snake People",
	"description": "Replaces the text 'Millennial' with 'Snake People'.",
	"author": "Eric Bailey",
	"version": "2.0",

	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"exclude_matches": ["*://*/*millennials-to-snake-people*"],
			"all_frames": true,
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}
